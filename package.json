{
  "name": "pixiv-filter-for-wxt",
  "description": "PixivでNG登録したユーザーやタグの作品を非表示にします。",
  "private": true,
  "version": "1.6.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm wxt\" \"pnpm esbuild\"",
    "dev:firefox": "wxt -b firefox",
    "dev:brave": "wxt -b brave",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "esbuild": "node esbuild.config.mjs"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/chrome": "^0.1.24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@wxt-dev/module-react": "^1.1.5",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "wxt": "^0.20.11"
  }
}
