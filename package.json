{
  "name": "pixiv-filter-for-wxt",
  "description": "PixivでNG登録したユーザーやタグの作品を非表示にします。",
  "private": true,
  "version": "1.6.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm wxt\" \"pnpm esbuild\"",
    "dev:firefox": "wxt -b firefox",
    "dev:brave": "wxt -b brave",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "esbuild": "node esbuild.config.mjs"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/chrome": "^0.1.6",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/wicg-file-system-access": "^2023.10.6",
    "@wxt-dev/module-react": "^1.1.4",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "wxt": "^0.20.10"
  }
}
